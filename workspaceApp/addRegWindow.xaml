        <Button x:Name="addRegionButton" Content="Добавить" HorizontalAlignment="Left" Height="37" Margin="10,44,0,0" VerticalAlignment="Top" Width="69" Click="Button_Click"/>
        <TextBox x:Name="newRegion" HorizontalAlignment="Left" Height="22" Margin="10,17,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="188"/>
