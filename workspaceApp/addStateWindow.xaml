<Window x:Class="workspaceApp.addStateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:workspaceApp"
        mc:Ignorable="d"
        Title="addStateWindow" Height="170" Width="290">
    <Grid Height="160" VerticalAlignment="Top">
        <ComboBox x:Name="regionSelect" HorizontalAlignment="Left" Height="27" Margin="22,8,0,0" VerticalAlignment="Top" Width="236" IsEditable="True"/>
        <TextBox x:Name="addState" HorizontalAlignment="Left" Height="27" Margin="87,51,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171"/>
        <Button x:Name="addButton" Content="Добавить" HorizontalAlignment="Left" Height="31" Margin="22,95,0,0" VerticalAlignment="Top" Width="95" Click="addButton_Click"/>
        <ComboBox x:Name="prfx" HorizontalAlignment="Left" Height="27" Margin="22,51,0,0" VerticalAlignment="Top" Width="51" IsEditable="True"/>

    </Grid>
</Window>
